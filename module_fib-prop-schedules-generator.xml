  <path id="fib-prop-schedules-generator.module.sourcepath">
    <dirset dir="${module.fib-prop-schedules-generator.basedir}/../../proyecto">
      <include name="FIB-PROP-Schedules-Generator"/>
      <include name="FIB-PROP-Schedules-Generator/data"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.fib-prop-schedules-generator" depends="compile.module.fib-prop-schedules-generator.production,compile.module.fib-prop-schedules-generator.tests" description="Compile module FIB-PROP-Schedules-Generator"/>
  
  <target name="compile.module.fib-prop-schedules-generator.production" depends="register.custom.compilers" description="Compile module FIB-PROP-Schedules-Generator; production classes">
    <mkdir dir="${fib-prop-schedules-generator.output.dir}"/>
    <javac2 destdir="${fib-prop-schedules-generator.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.fib-prop-schedules-generator}/javac">
      <compilerarg line="${compiler.args.fib-prop-schedules-generator}"/>
      <bootclasspath refid="fib-prop-schedules-generator.module.bootclasspath"/>
      <classpath refid="fib-prop-schedules-generator.module.production.classpath"/>
      <src refid="fib-prop-schedules-generator.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.fib-prop-schedules-generator"/>
    </javac2>
    
    <copy todir="${fib-prop-schedules-generator.output.dir}">
      <fileset dir="${module.fib-prop-schedules-generator.basedir}">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.fib-prop-schedules-generator.basedir}/data">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.fib-prop-schedules-generator.tests" depends="register.custom.compilers,compile.module.fib-prop-schedules-generator.production" description="compile module FIB-PROP-Schedules-Generator; test classes" unless="skip.tests"/>
  
  <target name="clean.module.fib-prop-schedules-generator" description="cleanup module">
    <delete dir="${fib-prop-schedules-generator.output.dir}"/>
    <delete dir="${fib-prop-schedules-generator.testoutput.dir}"/>
  </target>
